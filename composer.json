{
    "name": "sade/sade",
    "description": "Create PHP components",
    "license": "MIT",
    "keywords": [
        "php",
        "components",
        "twig"
    ],
    "autoload": {
        "psr-4": {
            "Sade\\": "src/"
        }
    },
    "require": {
        "twig/twig": "^2.0",
        "sabberworm/php-css-parser": "^8.2",
        "hashids/hashids": "^2.0",
        "webmozart/glob": "^4.1",
        "league/climate": "^3.4"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.3",
        "phpunit/phpunit": "^6.5"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["bin/sade"]
}
